#include<iostream.h>
#include<math.h>
#include<iomanip.h>
int main()
{
  cout<<fixed<<setprecision(2);
  cout<<"sin(100) = "<<sin(100.00)<<endl;   //computing sine value
  cout<<"cos(100) = "<<cos(100.00)<<endl;   //computing cosine value 
  cout<<"tan(100) = "<<tan(100.00)<<endl;   //computing tangent value
  cout<<"7 to the power 6 ="<<pow(7.0,6.0"<<endl;
  cout<<"log10 (10)="<<log10(10.00)<<endl;
  cout<<"sqrt(2)="<<sqrt(2)<<endl;
  return 0;
}
