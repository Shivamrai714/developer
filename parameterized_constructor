#include<iostream>
using namespace std;

class Point
{
	int x,y;
	public:
	Point(int a,int b)
	{
			x=a;
			y=b;	
	}	
	void display()
	{
		cout<<"x="<<x<<endl;
		cout<<"y="<<y<<endl;
	}
};

int main()
{
	Point p1(1,1);//invoke para.. const..
	Point p2(5,10);
	cout<<"Point p1"<<endl;
	p1.display();
	cout<<"Point p2"<<endl;
	p2.display();
	return 0;
}
