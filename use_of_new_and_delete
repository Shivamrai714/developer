#include<iostream.h>
#include<conio.h>
using namespace std;
int main()
{
  int *arr;
  int size;
  cout<<"Enter the size of the integer array";
  cin>>size;
  cout<<"creating an array of size"<<size;
  arr=new int(size);
  cout<<"dynamic allocation of memory for array is successful";
  delete arr;
  return 0;
}
