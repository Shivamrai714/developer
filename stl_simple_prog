#include<bits/stdc++.h>
int main()
{
	int n;
	cout<<"Enter the number of elements\t";
	cin>>n;
	int a[n];
	for(int i=0;i<n;i++)
		cin>>a[i];
	sort(a,a+4);
	for(int i=0;i<n;i++)
		cout<<a[i]<<" ";
	cout<<endl;
  if(binary_search(a,a+4,8))
		cout<<"Binary";
	else
		cout<<"not present ";
	return 0;
	
}
