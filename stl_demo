#include<bits/stdc++.h>
using namespace std;
int main()
{
	int arr[5];
	cout<<"Enter array elements "<<endl;
	for(int i=0;i<5;i++)
		cin>>arr[i];
	cout<<"elements of array is "<<endl;
	for(int i=0;i<5;i++)
		cout<<arr[i]<<" ";
	sort(arr,arr+5);
	cout<<"\nsorted elements is "<<endl;
	for(int i=0;i<5;i++)
		cout<<arr[i]<<" ";
	for(int i=0;i<5;i++)	
	if(binary_search(arr,arr+5,45))
	{
		cout<<"\nPresent"<<endl;
		break;
	}
	else
		cout<<"not present "<<endl;
	
	return 0;
}
