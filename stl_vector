#include<bits/stdc++.h>
using namespace std;

int main()
{
	vector <int> v;
	v.push_back(10);
	v.push_back(100);
	v.push_back(1000);
	v.push_back(10000);
	cout<<"vector size = "<<v.size()<<endl;
	for(int i=0;i<v.size();i++)
		cout<<"vector "<<i<<" "<<v.at(i)<<endl;
	v.erase(v.begin()+3);
	cout<<endl<<endl;
	for(int i=0;i<v.size();i++)
		cout<<"vector "<<i<<" "<<v.at(i)<<endl;
	return 0;
	
}
